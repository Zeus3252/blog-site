{
	"env": {
			"browser": true,
			"commonjs": true,
			"es2021": true
	},
	"extends": [
			"standard-with-typescript",
			"prettier" 
	],
	"plugins": ["prettier"],
	"overrides": [
	],
	"parserOptions": {
			"ecmaVersion": "latest"
	},
	"rules": {
			"prettier/prettier": "error",
			"@typescript-eslint/no-var-requires": 0,
			"@typescript-eslint/strict-boolean-expressions": 0,
			"@typescript-eslint/no-misused-promises": "off"
			
	}
}
